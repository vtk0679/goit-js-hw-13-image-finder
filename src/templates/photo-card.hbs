{{#each this}}
<li class="gallery__item photo-card">
<a class="gallery__link" href="{{original}}">
<img class="gallery__image" src="{{webformatURL}}" alt="{{tags}}" data-source="{{largeImageURL}}" />
</a>
    <div class="stats">
        <p class="stats-item">
            <i class="material-icons">thumb_up</i>
            {{likes}}
        </p>
        <p class="stats-item">
            <i class="material-icons">visibility</i>
            {{views}}
        </p>
        <p class="stats-item">
            <i class="material-icons">comment</i>
            {{comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons">cloud_download</i>
            {{downloads}}
        </p>
    </div>
   
</li>
{{/each}}